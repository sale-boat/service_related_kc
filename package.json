{
  "name": "amazon-product-carousel",
  "version": "1.0.0",
  "description": "Visual and functional clone of the amazon product ad carousel",
  "main": "index.js",
  "scripts": {
    "seed": "node server/helpers/seedProducts.js && node server/helpers/seedJunction.js && echo 'database built'",
    "dev-build": "mkdir dist; cp ./src/index.html ./dist/index.html && webpack -d --watch",
    "dev-start": "nodemon",
    "start": "webpack -p && cp ./src/index.html ./dist/index.html && node index.js",
    "test": "jest"
  },
  "author": "Pablo Frias",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "faker": "^4.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "react-test-renderer": "^16.8.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.2",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "knex": "^0.16.3",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.9",
    "pg": "^7.8.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./client/src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    }
  }
}
